{
  "name": "OfficeOnlineWord",
  "version": "v0.0.1",
  "license": "MIT",
  "description": "your app description",
  "author": "simplyCodin <xyz@gmail.com>",
  "homepage": "https://github.com/SimplyCodin/OfficeOnlineWord",
  "url": "https://github.com/SimplyCodin/OfficeOnlineWord",
  "main": "main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "npm install && electron .",
    "pack": "build --dir",
    "dist": "build",
    "dist:ci": "build -w -l",
    "github": "github-release upload -o simplycodin -r OfficeOnlineWord -t ${npm_package_version} -n ${npm_package_name}-${npm_package_version} dist"
  },
  "build": {
    "appId": "io.atom.electron.word",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel"
    },
    "squirrelWindows": {
      "iconUrl": "https://raw.githubusercontent.com/SimplyCodin/OfficeOnlineWord/master/word.ico"
    }
  },
  "dependencies": {
    "electron-builder-squirrel-windows": "^19.21.1"
  },
  "devDependencies": {
    "electron": "latest",
    "electron-builder": "latest",
    "github-release-cli": "^0.3.0"
  },
  "optionalDependencies": {}
}
